title = {{Light Robots: Bridging the Gap between Microrobotics and Photomechanics in Soft Materials}},
url = {http://doi.wiley.com/10.1002/adma.201703554},
volume = {30},
year = {2018}
}
@article{Szell2018,
abstract = {2-Iodoethynylpyridine and 2-iodoethynyl-1-methyl-imidazole self-assemble under halogen-bonding control into discrete macro-cycles, viz. supramolecular triangles. Inspired by nature, supramolecular chemists assemble molecular building blocks into complex and often functional supramolecular architectures. To this purpose, a plethora of noncovalent interactions are available, spanning a wide range of strengths, specificities, and directionalities. 1,2 The high-fidelity assembly of small molecules into discrete supermolecules requires strong and directional interactions, such as metal coordination that takes advantage of the predictable nature of metal-ligand coordination and ligand ability to encode directionality. 3 A wide diversity of 2D (e.g., triangles, squares, rectangles, and rhomboids) and 3D systems, (e.g., pyramids, prisms, cubes, etc.) have been reported. 4-8 On the other hand, polygonal supramolecular architectures assembled via weaker interactions, such as hydrogen bonding, are much less common. Only a few examples of hydrogen-bonded supramolecular triangles have been reported, mostly relying on double hydrogen bonding patterns involving nucleotides. 9-14 A triangle may be prepared by the combination of three 601 donor units and three 1801 acceptors. Alternatively, the donor and acceptor units may be mounted over the same molecular scaffold at the angle of 601, and the triangular shape results from the high-fidelity self-complementary assembly. Inspired by this latter design, we reasoned that the halogen bond (XB), namely the noncovalent interaction involving halogen atoms as electrophilic sites, may possess the strength, specificity, and high directionality required to result in such self-assembly. 15-19 To the best of our knowledge, only two examples of halogen-bonded triangles have been reported in the literature. Bowling and co-workers 20 have exploited intramolecular XB between aryl halide donors and several neutral XB-acceptors, held in proximity by 1,2-aryldiyne linkers, to build-up a family of supramolecular triangular structures in the solid state. Furthermore, we found in the CSD ‡ that 2,4,5-triiodo-1-methyl-1H-imidazole 21 packs in the crystalline state into halogen-bonded triangular motifs (Scheme 1). These examples inspired our design towards larger halogen-bonded triangular macrocycles. We anticipate the larger size to yield accessible voids within the triangles, a clear advancement compared to the previous reports. Here we report that 2-iodoethynylpyridine and 2-iodoethynyl-1-methyl-imidazole self-assemble under halogen-bonding control forming discrete macrocycles, viz. supramolecular triangles. The design principles to obtain a triangular halogen-bonded macrocycle are: (i) starting building blocks with a rigid planar structure, (ii) self-complementary XB-donor and acceptor groups directed along the sides of an equilateral triangle, Scheme 1 The trimeric triangular motif observed in the crystal structure of 1-methyl-2,4,5-triiodoimidazole (left) and the molecular building blocks used in this study (right), bearing both XB-donor and XB-acceptor sites within the same molecular scaffold.},
author = {Szell, Patrick M.J. and Siiskonen, Antti and Catalano, Luca and Cavallo, Gabriella and Terraneo, Giancarlo and Priimagi, Arri and Bryce, David L. and Metrangolo, Pierangelo},
doi = {10.1039/c8nj00759d},
issn = {13699261},
journal = {New Journal of Chemistry},
number = {13},
pages = {10467--10471},
title = {{Halogen-bond driven self-assembly of triangular macrocycles}},
url = {http://xlink.rsc.org/?DOI=C8NJ00759D},
volume = {42},
year = {2018}
}
@article{Wani2018,
abstract = {Abstract A versatile, laser-projector-based method is demonstrated for programming alignment patterns into monolithic films of liquid crystal polymer networks. Complex images can be photopatterned into the polymer films with sub-100 µm resolution, using relatively short exposure times. The method is further used to devise both photochemically and photothermally driven actuators that can undergo distinct light-induced shape changes, dictated by the programmed alignment patterns. Deformation modes such as buckling and coiling, as well as miniature robotic devices such as a gripper and a light-responsive octopod, are demonstrated. The reported technique enables easy and cost-effective programmable actuation with relatively high throughput, thus significantly facilitating the design and realization of functional soft robotic actuators.},
author = {Wani, Owies M. and Zeng, Hao and Wasylczyk, Piotr and Priimagi, Arri},
doi = {10.1002/adom.201700949},
issn = {21951071},
journal = {Advanced Optical Materials},
keywords = {azobenzene,laser projectors,liquid crystal,patterning,photoactuation,photoalignment},
month = {jan},
number = {1},
pages = {1700949},
title = {{Programming Photoresponse in Liquid Crystal Polymer Actuators with Laser Projector}},
url = {http://doi.wiley.com/10.1002/adom.201700949},
volume = {6},
year = {2018}
}
@article{Poutanen2018,
abstract = {Photoisomerization of azobenzene derivatives is a versatile tool for devising light-responsive materials for a broad range of applications in photonics, robotics, microfabrication, and biomaterials science. Some applications rely on fast isomerization kinetics, while for others, bistable azobenzenes are preferred. However, solid-state materials where the isomerization kinetics depends on the environmental conditions have been largely overlooked. Herein, an approach to utilize the environmental sensitivity of isomerization kinetics is developed. It is demonstrated that thin polymer films containing hydroxyazobenzenes offer a conceptually novel platform for sensing hydrogen-bonding vapors in the environment. The concept is based on accelerating the thermal cis–trans isomerization rate through hydrogen-bond-catalyzed changes in the thermal isomerization pathway, which allows for devising a relative humidity sensor with high sensitivity and quick response to relative humidity changes. The approach is also applicable for detecting other hydrogen-bonding vapors such as methanol and ethanol. Employing isomerization kinetics of azobenzenes for vapor sensing opens new intriguing possibilities for using azobenzene molecules in the future.},
author = {Poutanen, Mikko and Ahmed, Zafar and Rautkari, Lauri and Ikkala, Olli and Priimagi, Arri},
doi = {10.1021/acsmacrolett.8b00093},
issn = {21611653},
journal = {ACS Macro Letters},
month = {mar},
number = {3},
pages = {381--386},
title = {{Thermal Isomerization of Hydroxyazobenzenes as a Platform for Vapor Sensing}},
url = {http://pubs.acs.org/doi/10.1021/acsmacrolett.8b00093},
volume = {7},
year = {2018}
}
