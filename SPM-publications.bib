volume = {28},
year = {2016}
}
@article{Poutanen2016,
abstract = {{\textcopyright} 2016 American Chemical Society. Light-responsive supramolecular self-assemblies exhibit interplay between order and dynamics of the self-assembling motifs, through which the thermal isomerization rate of azobenzene chromophores can be tuned by orders of magnitude. By using supramolecular complexes of 4-(4-alkylphenylazo)phenols hydrogen-bonded to poly(4-vinylpyridine) as model systems, we demonstrate that the thermal isomerization rate of the hydroxyazobenzene derivatives increases 5700-fold when the material undergoes a transformation from a disordered, low-azobenzene-concentration state to a high-concentration state exhibiting lamellar, smectic-like self-assembly. Drastically smaller thermal isomerization rates are observed in disordered structures. This allows us to attribute the change to a combination of increased number density of the hydroxyazobenzenes inducing plasticization, and cooperativity created by the chromophore-chromophore interactions through self-assembled molecular order and alignment. Our results pinpoint the importance of molecular self-assembly and intermolecular interactions in modifying the dynamics in supramolecular complexes in a controlled manner. We foresee this to be important in light-controlled dynamic materials.},
author = {Poutanen, Mikko and Ikkala, Olli and Priimagi, Arri},
doi = {10.1021/acs.macromol.6b00562},
issn = {0024-9297},
journal = {Macromolecules},
