number = {1},
pages = {96--99},
title = {{Location of the Azobenzene moieties within the cross-linked liquid-crystalline polymers can dictate the direction of photoinduced bending}},
url = {http://pubs.acs.org/doi/10.1021/mz200056w},
volume = {1},
year = {2012}
}
